{
  "manifest_version": 3,
  "permissions": ["storage"],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*", "*://m.youtube.com/*"],
      "all_frames": true,
      "js": ["src/contentScript/index.ts"]
    }
  ]
}
